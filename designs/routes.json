{
    "routes": [
        {
            "path": "home",
            "component": "simplepageComponent"
        },
        {
            "path": "form",
            "component": "formpageComponent"
        },
        {
            "path": "table",
            "component": "tablepageComponent"
        },
        {
            "path": "chart",
            "component": "chartpageComponent"
        },
        {
            "path": "sidenav",
            "component": "sidenavComponent"
        },
        {
            "path": "''",
            "redirectTo": "home",
            "pathMatch": "full",
            "default": true
        },
        {
            "path": "**",
            "component": "PageNotFoundComponent"
        }
    ]
}
